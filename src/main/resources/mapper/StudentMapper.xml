<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xhu.onlinecourse.mapper.StudentMapper">
    <insert id="studentInsert" parameterType="Student">
        insert into student(student_name,student_password,student_gender,student_birthday,student_phonenum,student_email,student_address)
        values(#{studentName},#{studentPassword},#{studentGender},#{studentBirthday},#{studentPhonenum},#{studentEmail},#{studentAddress});
    </insert>

    <select id="studentList" resultType="Student">
        select * from student
    </select>

</mapper>